@using Skill_CodeFirstEntity.Models.Sınıflar
@model List<YETENEKLER>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutNavbar.cshtml";
}

<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>YETENEK</th>
        <th>DEĞER</th>
        <th>SİL</th>
        <th>GÜNCELLE</th>
    </tr>
    @foreach (var x in Model)
    {
        <tr>
            <td>@x.ID</td>
            <td>@x.ACIKLAMA</td>
            <td>@x.DEGER</td>
            <td><a href="/Admin/YetenekSil/@x.ID" class="btn btn-danger">Sil</a></td>
            <td><a href="/admin/YetenekGuncelle/@x.ID" class="btn btn-success">Güncelle</a></td>
        </tr>

    }



</table>

